{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "single"],
        "indent": ["error","tab"],
        "no-var": "error",
        "no-multi-spaces": "error",
        "no-trailing-spaces": "error"
    }
}
// More Rules can be found at: https://eslint.org/docs/latest/rules
